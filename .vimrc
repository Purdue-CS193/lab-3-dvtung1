set number
set background=dark
set number
set visualbell
set t_vb=
set autoindent
set smartindent
set expandtab
set shiftwidth=4
set softtabstop=4
nnoremap <C-Left> :tabprevious<CR>
nnoremap <C-Right> :tabnext<CR>
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
